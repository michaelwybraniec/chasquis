{
  "name": "@nuxt/ui-pro",
  "description": "Ship beautiful web applications at the speed of light with Nuxt UI Pro.",
  "type": "module",
  "version": "1.7.0",
  "packageManager": "pnpm@9.15.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt/ui-pro.git"
  },
  "main": "./nuxt.config.ts",
  "files": [
    "app",
    "assets",
    "components",
    "composables",
    "lib",
    "modules",
    "plugins",
    "types",
    "app.config.ts",
    "nuxt.config.ts",
    "tailwind.config.ts"
  ],
  "theme": {
    "env": "NUXT_UI_PRO_LICENSE",
    "link": "https://ui.nuxt.com/pro/pricing"
  },
  "scripts": {
    "dev": "DEV=true nuxi dev .docs",
    "build": "DEV=true nuxi generate .docs",
    "play": "nuxi dev .playground",
    "prepare": "DEV=true nuxi prepare .docs",
    "lint": "eslint .",
    "typecheck": "nuxi typecheck",
    "release": "release-it",
    "test": "TZ=Europe/Paris vitest"
  },
  "dependencies": {
    "@iconify-json/vscode-icons": "^1.2.10",
    "@nuxt/ui": "^2.21.0",
    "@vueuse/core": "^12.4.0",
    "defu": "^6.1.4",
    "git-url-parse": "^16.0.0",
    "ofetch": "^1.4.1",
    "parse-git-config": "^3.0.0",
    "pathe": "^2.0.1",
    "pkg-types": "^1.3.0",
    "tailwind-merge": "^2.6.0",
    "vue3-smooth-dnd": "^0.0.6"
  },
  "devDependencies": {
    "@nuxt/content": "^2.13.4",
    "@nuxt/eslint-config": "^0.7.5",
    "@nuxt/image": "^1.9.0",
    "@nuxt/test-utils": "^3.15.4",
    "@release-it/conventional-changelog": "^10.0.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.18.0",
    "happy-dom": "^14.12.3",
    "nuxt": "^3.15.1",
    "release-it": "^18.1.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8",
    "vitest-environment-nuxt": "^1.0.1",
    "vue-tsc": "^2.1.10",
    "zod": "^3.24.1"
  },
  "resolutions": {
    "@nuxt/ui-pro": "workspace:*",
    "vue-tsc": "2.1.10",
    "typescript": "5.6.3"
